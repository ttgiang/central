								<!-- progress tab -->
								<TABLE cellSpacing=0 cellPadding=5 width="100%" border=0>
									<TBODY>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Proposer:</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[0]%></TD>
										</TR>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Progress:</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[1]%></TD>
										</TR>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Modify Date:</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[2]%></TD>
										</TR>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Comments<br>(reasons for course action):</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[8].replace("\r\n","<br>")%></TD>
										</TR>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Effective Term:</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[3]%></TD>
										</TR>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Next Review Date:</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[4]%></TD>
										</TR>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Approved Date:</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[6]%></TD>
										</TR>
										<TR>
											<TD class="textblackTH" width="15%" nowrap>Last Updated:</TD>
											<TD class="dataColumn"  width="85%"><%=statusTab[7]%></TD>
										</TR>
									</TBODY>
								</TABLE>
